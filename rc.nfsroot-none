#!/bin/bash
#
# rc.nfsroot-none - nfsroot startup (none method)
#
prog=rc.nfsroot-none
testfile=/.foo
INITPROG="/sbin/init"
configfile=/etc/sysconfig/nfsroot
if [ -f $configfile ]; then
   . $configfile
fi
export PATH=/bin:/sbin:/usr/bin:/usr/sbin

if ! touch ${testfile} 2>/dev/null; then
   echo "${prog}: root file system is read-only" >&2
   return 1
fi
rm -f ${testfile}

echo "$prog: passing control to ${INITPROG}" 2>&1
exec ${INITPROG} "$@" <dev/console >dev/console 2>&1
return 1
